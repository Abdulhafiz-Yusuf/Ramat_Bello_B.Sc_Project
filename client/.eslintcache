cle border'\n                                                    style={{ height: '100px', width: '100px' }} />\n                                            </td>\n                                            <td>{inmate.f_name} {inmate.m_name} {inmate.l_name}</td>\n                                            <td>{inmate.code}</td>\n                                            <td >\n                                                <Button\n\n                                                    onClick={() => dispatch(viewPageAction('profile', inmate))}\n                                                    className='ml-2 text-light bg-success font-weight-bold'>View\n                                                </Button>\n                                                {/* <Button\n\n                                        onClick={() => dispatch(viewPageAction('generatePass'))}\n                                        className='ml-2 text-light bg-success font-weight-bold'>Generate Pass</Button> */}\n\n                                            </td>\n                                        </tr>\n                                    )\n                                })\n\n                            }\n                        </tbody>\n                    </Table>\n                    :\n                    <div>\n\n                    </div>\n            }\n\n        </div >\n    )\n}\n\nexport default SearchImmate\n","/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/Guest/GeneratePass.js",["162","163","164","165","166"],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/Guest/RegGuest.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/components/utility/useWindowSize.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/Immate/RegInmate.js",["167"],"import React, { useContext, useState, } from 'react'\nimport { Card, Button, Form, FormGroup, Label, Input } from 'reactstrap';\n// import { } from '../../../ContextAPI/actions/inmateActions'\nimport { globalStore } from '../../../ContextAPI/globalStore'\n//DATEPICKER AND ITS CSS\nimport DatePicker from \"react-datepicker\";\nimport \"react-datepicker/dist/react-datepicker.css\";\n\nimport { firebaseImageUploadforNewInmate, RegisterInmate } from '../../../ContextAPI/actions/inmateActions';\n\nimport {\n    NaijaStates, NaijaLGA\n} from './stateLGAData';\n\n/*==========================\nREGISTRATION COMPLETION PAGE\n=============================*/\n\nexport default function RegInmate(props) {\n\n    const { dispatch } = useContext(globalStore)\n\n\n    let formData = new FormData();\n\n    //Firebase file upload states\n    const [Url, setUrl] = useState()\n    const [progress, setProgress] = useState(0)\n    const [selectedFile, setSelectedFile] = useState({ name: '', file: '' })\n    const [prevPicName, setPrevPicName] = useState()\n\n    const [profile, setProfile] = useState({\n        fName: '',\n        lName: '',\n        mName: '',\n        gender: 'Male',\n        phone: '',\n        dob: new Date(),\n        inmate_loc_state: 'Abia',\n        loc_lga: NaijaLGA['Abia'][0],\n        homeAdd: '',\n        email: '',\n        cCenter: '',\n        code: '',\n        doi: new Date(),\n        dor: new Date(),\n        crime: ''\n\n    })\n\n    let LGAs = NaijaLGA[profile.inmate_loc_state]\n\n    const handleChange = (e) => {\n        const value = e.target.value\n        setProfile({\n            ...profile,\n            [e.target.name]: value\n        })\n        console.log(value)\n    }\n\n    const onSubmit = (e) => {\n        e.preventDefault()\n        const dataToSubmit = {\n            fName: profile.fName,\n            lName: profile.lName,\n            mName: profile.mName,\n            gender: profile.gender,\n            phone: profile.phone,\n            dob: profile.dob,\n            inmate_loc_state: profile.inmate_loc_state,\n            loc_lga: profile.loc_lga,\n            homeAdd: profile.homeAdd,\n            email: profile.email,\n            cCenter: profile.cCenter,\n            code: profile.code,\n            doi: profile.doi,\n            dor: profile.dor,\n            crime: profile.crime,\n            iPicUrl: Url,\n            iPicName: selectedFile.name\n        }\n\n        if (\n            dataToSubmit.fName === '' ||\n            dataToSubmit.lName === '' ||\n            dataToSubmit.mName === '' ||\n            dataToSubmit.phone === '' ||\n            dataToSubmit.homeAdd === '' ||\n            dataToSubmit.email === '' ||\n            dataToSubmit.cCenter === '' ||\n            dataToSubmit.code === '' ||\n            dataToSubmit.crime === ''\n        ) {\n            alert('All fields are required, Field cannot be empty')\n        }\n        else if (!Url) {\n            alert('Inmate Passport is required')\n        }\n        else {\n\n            RegisterInmate(dispatch, dataToSubmit)\n\n        }\n    }\n    console.log(Url)\n\n    const onImageUpload = () => {\n\n        // const config = {\n        //     header: { 'content-type': 'multipart/form-data' }\n        // }\n        formData.append(\"file\", selectedFile.file)\n        //save the Image we chose inside the Node Server \n        let update = false\n        firebaseImageUploadforNewInmate(dispatch, selectedFile, setUrl, setProgress, prevPicName, setPrevPicName)\n        // uploadImage(dispatch, formData, config, setImage, setInmatePicName)\n    }\n\n    const onfileSelect = (e) => {\n        e.preventDefault()\n        setSelectedFile({\n            name: e.target.files[0].name,\n            file: e.target.files[0],\n            img: URL.createObjectURL(e.target.files[0])\n        })\n\n    }\n\n    return (\n        <div className='w-75'>\n            <div style={{ height: '10px' }}></div>\n            <Card className='container w-100 shadow-lg p-3'>\n               