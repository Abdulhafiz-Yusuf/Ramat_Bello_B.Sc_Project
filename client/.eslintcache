             <GeneratePass />\n                                :\n                                stateViewPage === 'regVisitor'\n                                    ?\n                                    <RegGuest />\n                                    :\n                                    stateViewPage === 'profile'\n                                        ?\n                                        <Profile />\n                                        :\n                                        <SearchImmate />\n\n                        }\n                    </div >\n                </div>\n            </div >\n\n        </div>\n    )\n}\n\n\n","/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/components/Loading.js",[],["150","151"],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/Profile.js",["152","153","154","155","156"],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/DashBoardMenu.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/components/CardList.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/components/SearchBar.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/data.js",[],["157","158"],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/components/Card.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/components/LoadScreen.js",["159","160"],"import React from 'react'\nimport { Alert, Label, Modal, Spinner } from 'reactstrap';\n\nfunction LoadScreen({ size, text, height }) {\n    return (\n        <div\n            style={{ height: height }}\n            className='container d-flex flex-column justify-content-center align-items-center' >\n            <Label style={{ fontWeight: 'bold', }}>{text ? text : 'Loading ....'}\n            </Label>\n            <Spinner size={size || \"large\"} color=\"primary\" />\n        </div>\n\n    );\n};\n\n\n\n\nexport default LoadScreen;","/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/ContextAPI/actions/UserActions.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/ContextAPI/globalStore.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/ContextAPI/rootReducer.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/ContextAPI/types.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/components/Carousel.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/About.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Login.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/RegisterImmate.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/Immate/SearchImmate.js",["161"],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/Guest/GeneratePass.js",["162"],"import React, {\n    useState,\n    useContext\n} from 'react'\nimport { Table, Card, Button, Form, FormGroup, Label, Input } from 'reactstrap';\n\nimport \"react-datepicker/dist/react-datepicker.css\";\nimport { globalStore } from '../../../ContextAPI/globalStore'\nimport { viewPageAction } from '../../../ContextAPI/actions/UserActions';\n/*==========================\nREGISTRATION COMPLETION PAGE\n=============================*/\n\nexport default function GeneratePass(props) {\n\n    const [profile, setProfile] = useState({\n        fName: '',\n        lastName: '',\n        mName: '',\n        gender: '',\n        phone: '',\n        dob: new Date(),\n        homeAdd: '',\n        emailAdd: '',\n        nameOfCorrCenter: '',\n        cellCode: '',\n        doi: new Date(), //date of imprisonment\n        dor: new Date(), //date of release\n        natureOfCrime: ''\n\n    })\n\n    const handleGuestChange = (e) => {\n        const value = e.target.value\n        setProfile({\n            ...profile,\n            [e.target.name]: value\n        })\n        console.log(value)\n    }\n    const onSubmit = (e) => {\n        e.preventDefault()\n        const dataToSubmit = {\n            email: 'me@gmail.com',\n            email_verified: false,\n            username: profile.username,\n            phone: profile.phone,\n            user_loc_state: profile.user_loc_state,\n            loc_lga: profile.loc_lga,\n            donor: profile.donor,\n            bg: profile.bg\n        }\n        // dispatch(completeRegistration(dataToSubmit))\n        if (dataToSubmit.username === '' || dataToSubmit.phone === '') {\n            alert('Field cannot be empty')\n        }\n        else {\n\n        }\n    }\n\n\n\n    // searchInamate\n    const [searchId, setsearchId] = useState()\n    const [SearchType, setSearchType] = useState('1')\n\n    const { dispatch } = useContext(globalStore)\n    const handleChange = (e) => {\n        const value = e.target.value\n        setsearchId(value)\n\n        console.log(value)\n    }\n\n\n    const onSearch = (e) => {\n        e.preventDefault()\n        const dataToSubmit = {\n            searchId: searchId\n        }\n        // dispatch(completeRegistration(dataToSubmit))\n        if (dataToSubmit.searchId === '') {\n            alert('Field cannot be empty')\n        }\n        else {\n\n        }\n    }\n\n    const bgHanleChange = (e) => {\n        const value = e.target.value\n        setSearchType(value)\n        console.log(value)\n    }\n\n\n\n    return (\n        <div className='w-75'>\n            <div style={{ height: '10px' }}></div>\n\n\n            <div className='d-flex flex-column align-items-center justify-content-lg-center '>\n                <h2 className='text-success font-weight-bold'>GENERATE GATE PASS</h2>\n                <h5 className='text-success text-center font-italic font-weight-bold'>\n                    Please generate gate pass by searching for an inmate and providing\n                    visitor's information below.\n                </h5>\n            </div>\n            <Card className='container w-100 shadow-lg p-3 mb-5'>\n                <Form className='d-flex flex-column justify-content-start ' >\n                    <h4 className=' text-success text-center font-weight-bold'>Search Inmate</h4>\n                    <FormGroup tag=\"fieldset\" className='d-flex flex-row' >\n                        {\n                            [\n                                { label: 'Search by ID', key: '1' },\n                                { label: 'Search by Name', key: '2' }\n\n                            ].map((option, index) => {\n                                return (\n                                    <FormGroup check>\n                                        <Label check className='text-success pl-5'>\n                                            <Input id={index} type=\"radio\" value={option.key} onChange={bgHanleChange} checked={option.key === SearchType} />\n                                            {option.label}\n                                        </Label>\n                                    </FormGroup>\n                                )\n                            })\n                        }\n                    </FormGroup>\n                    < FormGroup className='mr-3'>\n                        <Input\n                            type=\"text\"\n                            name=\"searchId\"\n                            value={searchId}\n                            onChange={handleGuestChange}\n                            placeholder={SearchType === '1' ? \"Enter Inmate's ID Number \" : \"Enter Inmate's Name\"} />\n                    </FormGroup>\n\n                    <Button color='success'\n                        onClick={onSearch}\n                        className='' >\n                        Search</Button>\n                </Form >\n\n                <Table className='text-success text-center mt-5 w-100' bordered hover striped>\n                    <thead>\n                        <tr>\n\n                            <th>Image</th>\n                            <th>Name</th>\n                            <th>ID</th>\n                            <th>Actions</th>\n\n                        </tr>\n                    </thead>\n                    <tbody>\n                        {[1].map((inmate, index) => {\n                            return (\n                                < tr key={index}>\n                                    <td>\n                                        <img src={inmate.img} alt='Inmate Pic'\n                                            className='rounded-circle border'\n                                            style={{ height: '100px', width: '100px' }} />\n                                    </td>\n                                    <td>{inmate.loclga} </td>\n                                    <td>{inmate.qty}</td>\n                                    <td >\n                                        <Button\n\n                                            onClick={() => dispatch(viewPageAction('profile', inmate))}\n                                            className='ml-2 text-light bg-success font-weight-bold'>View </Button>\n                                        {/* <Button\n\n                                        onClick={() => dispatch(viewPageAction('generatePass'))}\n                                        className='ml-2 text-light bg-success font-weight-bold'>Generate Pass</Button> */}\n\n                                    </td>\n                                </tr>\n                            )\n                        })\n\n                        }\n                    </tbody>\n                </Table>\n\n            </Card>\n            <Card className='container w-100 shadow-lg p-3 '>\n\n                <Form>\n                    <h4 className=' text-success text-center font-weight-bold mt-4'>Visitor Information</h4>\n                    <FormGroup >\n                        <Label for=\"fName\">First Name</Label>\n                        <Input type=\"text\" name=\"fName\" value={profile.fName} onChange={handleGuestChange} placeholder=\"First Name\" />\n                    </FormGroup>\n                    <FormGroup>\n                        <Label for=\"lastName\">Last Name</Label>\n                        <Input type=\"text\" name=\"lastName\" value={profile.sName} onChange={handleGuestChange} placeholder=\"Last Name\" />\n                    </FormGroup>\n                    <FormGroup>\n                        <Label for=\"mName\">Middle Name</Label>\n                        <Input type=\"text\" name=\"mName\" value={profile.mName} onChange={handleGuestChange} placeholder=\"Middle Name\" />\n                    </FormGroup>\n\n                    <FormGroup>\n                        <Label for=\"gender\">Gender</Label>\n                        <Input type=\"select\" name=\"gender\" value={profile.gender} onChange={handleGuestChange}>\n                            {['Male', 'Female'].map((gender, index) => (\n                                <option key={index} value={gender}> {gender}</option>\n                            ))\n                            }\n                        </Input>\n                    </FormGroup>\n\n                    <FormGroup>\n                        <Label for=\"phone\">phone</Label>\n                        <Input type=\"text\" name=\"phone\" value={profile.phone} onChange={handleGuestChange} placeholder=\"080xxxxxxx\" />\n                    </FormGroup>\n\n                    <FormGroup>\n                        <Label for=\"homeAdd\">Home Address</Label>\n                        <Input type=\"text\" name=\"homeAdd\" value={profile.homeAdd} onChange={handleGuestChange} placeholder=\"Home Address\" />\n                    </FormGroup>\n\n                </Form>\n\n            </Card >\n\n            <div className='d-flex justify-content-lg-center '>\n                <Button color='success' className='font-weight-bold mt-3' onClick={onSubmit}>Generate Gate Pass</Button>\n            </div>\n            <div style={{ height: '20px' }}></div>\n        </div>\n    )\n}\n\n\n","/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/Guest/RegGuest.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/components/utility/useWindowSize.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/Immate/RegInmate.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/components/CardComponent.js",["163"],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/services/FirebaseConfig.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/ContextAPI/actions/inmateActions.js",[],{"ruleId":"164","replacedBy":"165"},{"ruleId":"166","replacedBy":"167"},{"ruleId":"168","severity":1,"message":"169","line":20,"column":10,"nodeType":"170","messageId":"171","endLine":20,"endColumn":14},{"ruleId":"168","severity":1,"message":"172","line":4,"column":8,"nodeType":"170","messageId":"171","endLine":4,"endColumn":16},{"ruleId":"168","severity":1,"message":"173","line":2,"column":39,"nodeType":"170","messageId":"171","endLine":2,"endColumn":48},{"ruleId":"168","severity":1,"message":"174","line":10,"column":10,"nodeType":"170","messageId":"171","endLine":10,"endColumn":26},{"ruleId":"168","severity":1,"message":"175","line":14,"column":13,"nodeType":"170","messageId":"171","endLine":14,"endColumn":18},{"ruleId":"168","severity":1,"message":"176","line":1,"column":17,"nodeType":"170","messageId":"171","endLine":1,"endColumn":25},{"ruleId":"168","sev