tion user={stateUser[0]} />\n                    :\n                    !stateUser ?\n\n                        <Loading />\n                        :\n                        stateUser.length > 0 &&\n                        displayUserProfile()\n\n\n            }\n\n        </div>\n    )\n}\n\n\n","/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/components/Loading.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/appStore/_reducers/index.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/Profile.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/BookingPage.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/PaymentPage.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/RegCompletion.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/views/Dashboard/DashBoardMenu.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/components/CardList.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/components/SearchBar.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/appStore/_actions/BloodBankAction.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/appStore/_actions/userAction.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/appStore/_reducers/BloodBankReducer.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/appStore/_reducers/userReducer.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/data.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_Bello_B.Sc_Project/client/src/components/Card.js",[],"/home/abdulhafiz/PROJECTS/REACT_PROJECTS/Ramat_B